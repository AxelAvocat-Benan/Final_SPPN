<p id="notice"><%= notice %></p>

<h1>Memberships</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Diplome</th>
      <th>Age</th>
      <th>Activity</th>
      <th>Massif</th>
      <th>Periode</th>
      <th>Passmorgiou</th>
      <th>Passsormiou</th>
      <th>Siret</th>
      <th>Description</th>
      <th>User</th>
      <th>Photo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @memberships.each do |membership| %>
      <tr>
        <td><%= membership.first_name %></td>
        <td><%= membership.last_name %></td>
        <td><%= membership.diplome %></td>
        <td><%= membership.age %></td>
        <td><%= membership.activity %></td>
        <td><%= membership.massif %></td>
        <td><%= membership.periode %></td>
        <td><%= membership.passmorgiou %></td>
        <td><%= membership.passsormiou %></td>
        <td><%= membership.siret %></td>
        <td><%= membership.description %></td>
        <td><%= membership.user %></td>
        <td><%= membership.photo %></td>
        <td><%= link_to 'Show', membership %></td>
        <td><%= link_to 'Edit', edit_membership_path(membership) %></td>
        <td><%= link_to 'Destroy', membership, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Membership', new_membership_path %>
