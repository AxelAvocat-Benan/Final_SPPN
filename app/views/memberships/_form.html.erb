<%= simple_form_for(@membership) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :diplome %>
    <%= f.input :age %>
    <%= f.input :activity %>
    <%= f.input :massif %>
    <%= f.input :periode %>
    <%= f.input :passmorgiou %>
    <%= f.input :passsormiou %>
    <%= f.input :siret %>
    <%= f.input :description %>
    <%= f.association :user %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
